# Neo4j Aura Connections (Required)
NEO4J_URL=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password

# SQLite Database Configuration (Required)
DATABASE_PATH=./ito.db

# Authentication Settings (Required)
SECRET_KEY=your-secret-key-change-in-production
FIRST_ADMIN_USER=admin
FIRST_ADMIN_PASSWORD=your-secure-admin-password

# Optional Settings
# ALGORITHM=HS256
# ACCESS_TOKEN_EXPIRE_MINUTES=30
# DEBUG=false
